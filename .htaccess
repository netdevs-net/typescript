<IfModule mod_headers.c>
    Header set Content-Security-Policy "default-src 'self'; script-src 'self' https://maps.googleapis.com; style-src 'self' 'unsafe-inline'; img-src 'self' https://maps.gstatic.com; font-src 'self'; connect-src 'self' https://maps.googleapis.com; frame-src 'self' https://maps.googleapis.com;"
</IfModule>
